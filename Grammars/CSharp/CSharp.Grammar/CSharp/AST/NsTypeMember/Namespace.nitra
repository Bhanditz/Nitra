using CSharp.Utils;
using Nitra.Runtime.Binding;

namespace CSharp
{
  declaration Namespace : NamespaceMember
  {
    Members.DefScopeIn = EnterNamespace(DefScopeIn, Path, this);
    DefScopeOut        = ExitNamespace(DefScopeIn, Members.DefScopeOut);

    Path            : Reference*;
    ExternAlias     : ExternAliasDirective*;
    UsingDirectives : UsingDirective*;
    Members         : NamespaceMember*;
  }
}