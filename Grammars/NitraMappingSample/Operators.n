public delegate BinaryOperator(value1 : double, value2 : double) : double; 

public delegate UnaryOperator(value : double) : double; 

public module Operators
{
  public Sum   : BinaryOperator = (a, b) => a + b;
  public Sub   : BinaryOperator = (a, b) => a - b;
  public Mul   : BinaryOperator = (a, b) => a * b;
  public Div   : BinaryOperator = (a, b) => a / b;
  public Plus  : UnaryOperator  = (x) => +x;
  public Minus : UnaryOperator  = (x) => -x;
}

