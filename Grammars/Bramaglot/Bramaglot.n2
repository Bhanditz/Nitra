namespace N2.Tests
{
  syntax module Barmaglot
  {
    using PrettyPrint;
    using Outline;
    using TokenNames;
    using StandardSpanClasses;
    using Whitespaces;
    using Identifiers;

    [StartRule, ExplicitSpaces]
    syntax Start = s Jabberwocky Jajabinks !Any;

    syntax Jajabinks  = (("ja" "ja")+ "binks")+;

    syntax Jabberwocky
    {
      | JabberWocky = "jabber" "wocky";
      | Jabber = "jabber";
      | Wocky = "wocky";
    }
  }
}
