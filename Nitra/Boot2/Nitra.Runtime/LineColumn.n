using Nemerle;
using Nemerle.Collections;
using Nemerle.Text;
using Nemerle.Utility;

using System;
using System.Collections.Generic;
using System.Linq;

namespace Nitra
{
  public enum OperatorType
  {
    | None
    | Unary
    | Binary
    | Ternary
    | NArry
  }
  
  [Record]
  public struct LineColumn
  {
    public Line   : int;
    public Column : int;

    public override ToString() : string { $"$Line, $Column" }
  }
}
