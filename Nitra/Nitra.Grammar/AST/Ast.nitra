using Nitra.AstUtils;
using Nitra.Runtime.Binding;

namespace Nitra.Ast
{
  abstract declaration Ast : NamespaceMemberDeclaration
  {
    sealed symbol
    {
      Kind      = "ast";
      SpanClass = NitraLangLanguage.AstSpanClass;
      FullName  = SymbolUtils.MakeFullName(this);

      out MemberTable : TableScope = TableScope(ToString());
    }
  }
}
