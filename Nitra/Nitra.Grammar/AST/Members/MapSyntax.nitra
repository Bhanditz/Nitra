using DotNet;

using Nitra.Runtime.Binding;

namespace Nitra.Ast
{
  abstract ast MapSyntax : BindableAst
  {
    Ast.Scope = Scope;
  stage 1:
    out RuleSymbol : SyntaxRuleSymbol;
    out AstSymbol  : AstSymbol;
    out AstRef     : Ref[AstSymbol] = this.Ast.Ref.Resolve();

    AstSymbol = AstRef.Symbol;

    Ast : QualifiedReference;
  }
}
