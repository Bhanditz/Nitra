using DotNet;

using Nitra.Runtime.Binding;

using System.Linq;

namespace Nitra.Ast
{
  declaration TopAst : Nitra.Ast.Ast, NamespaceMemberDeclaration
  {
    Members.Parent = Symbol;
  }

  ast TopMapSyntax : MapSyntax, NamespaceMember
  {
    Rule.Scope = Scope;
  stage 1:
    out RuleRef : Ref[SyntaxRuleSymbol] = this.Rule.Ref.Resolve();

    RuleSymbol = RuleRef.Symbol;

    Rule : QualifiedReference;
  }
}
