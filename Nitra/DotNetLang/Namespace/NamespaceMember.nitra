using Nitra;
using Nitra.Declarations;

using System.Linq;

namespace DotNet
{
  abstract ast NamespaceMember : BindableAst, ScopedAst
  {
    in DeclaredIn  : NamespaceSymbol;

  stage 1:
    inout Opening           : Scope;
    in    CurrentScope      : Scope;
    in    UsingAliasesScope : TableScope;

    ContainingTable = DeclaredIn.MemberTable;
  }
}
