CalcGrammar.Start
 Expr = CalcGrammar.Seq
  OpenBrace = '{'
  Exprs = 
  [
   CalcGrammar.Add
    Expr1 = CalcGrammar.Num
     Number = NumParser.Number
      number = '1'
    Operator = '+'
    Expr2 = CalcGrammar.Num
     Number = NumParser.Number
      number = '3'
   CalcGrammar.Add
    Expr1 = CalcGrammar.Num
     Number = NumParser.Number
      number = '2'
    Operator = '+'
    Expr2 = CalcGrammar.Num
     Number = NumParser.Number
      number = '3'
   CalcGrammar.Seq
    OpenBrace = '{'
    Exprs = 
    [
     CalcGrammar.Num
      Number = NumParser.Number
       number = '4'
     CalcGrammar.Sub
      Expr1 = CalcGrammar.Num
       Number = NumParser.Number
        number = '1'
      Operator = '-'
      Expr2 = CalcGrammar.Num
       Number = NumParser.Number
        number = '2'
    ]
    CloseBrace = '}'
  ]
  CloseBrace = '}'
