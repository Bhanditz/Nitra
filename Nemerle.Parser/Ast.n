using Nemerle;
using Nemerle.Collections;
using Nemerle.Text;
using Nemerle.Utility;

using System;
using System.Collections.Generic;
using System.Linq;

namespace Nemerle.Parser
{
  public class Ast : Located
  {
    public Errors : list[ErrorInfo] { get; }

    public this()
