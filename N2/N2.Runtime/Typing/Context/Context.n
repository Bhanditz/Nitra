using Nemerle;
using Nemerle.Collections;
using Nemerle.Text;
using Nemerle.Utility;

using System;
using System.Collections.Generic;
using System.Linq;

namespace N2.Typing
{
  [Record]
  public abstract class Context
  {
    public Parent : Context { get; }

    public virtual GetBinder[T](name : Ast) : IBinder[T]
      where T: Symbol
    {
      // if 
      Parent.GetBinder(name)
    }
  }
}
