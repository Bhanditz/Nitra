using Whitespaces;

syntax module A
{
  syntax A = "A";   //H:previous definition
  token  A = "A";   //E:The token rule 'A' already defined
}

syntax module B
{
  syntax B
  {
    regex X = "A";  //H:previous definition
    | X;            //E:The extension rule 'X' already defined
  }
}