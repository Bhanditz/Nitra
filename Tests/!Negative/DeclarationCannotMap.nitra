syntax module M1
{
  using Whitespaces;

  syntax R1 = R2?
  {
    map D1
    {
      R2Opt -> Items; // E: Expected AST type 'D2\*' got option
      R2Opt -> S;     // E: Rule 'M1\.R2' has no compatible AST mapping
    }
  }
 
  syntax R2 = "x"
  {
    map D2
    {
      A = 10;
    }
  }
}

ast D1
{
  Items : D2*;
  S     : D3?;
}

ast D2
{
  A : int;
}

ast D3
{
  S : Nitra.NSpan;
}
