syntax module M1
{
  using Whitespaces;

  syntax R1 = "x"
  {
    map D1
    {
      A = 10;
    } // E: Assignment of field 'B' is required
  }
  
  syntax R2
  {
    map D1;

    | X = "x"
      {
        map D1
        {
          A = 10;
        } // E: Assignment of field 'B' is required
      }
  }

  syntax R3 = "x"
  {
    map D2; // E: Assignment of field 'A' is required
  }
}

ast D1
{
  A : int;
  B : int;
}

ast D2
{
  A : int;
}
