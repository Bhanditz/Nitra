syntax module M1
{
  using Whitespaces;

  token Number = ['0'..'9']+;

  syntax Expr
  {
    | Number
    | Sum = Expr "+" Expr
      {
        precedence 10;
      }
    | Mul = Expr ^ 10 "*" Expr //E:You can't set a binding power if you set a precedence
      {
        precedence 20;
      }
  }
}
