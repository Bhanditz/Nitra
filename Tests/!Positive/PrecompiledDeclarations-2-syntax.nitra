// REFERENCE: PrecompiledDeclarations-1-syntax
using Nitra;

syntax module M1
{
  using Whitespaces;
  
  [StartRule]
  syntax Start = Item* !Any
  {
    map to CompilationUnit { Items -> Items; }
  }
  
  syntax Item
  {
    | A = "a"
    | B = "b"
  }
}

map syntax M1.Item -> Item;

map syntax M1.Item.A -> Item
{
  Content = this.Location.Span;
}

map syntax M1.Item.B -> Item
{
  Content = this.Location.Span;
}

syntax module M2
{
  using Whitespaces;
  using M1;

  extend syntax Item
  {
    | C = "c" { map to Item { Content = this.Location.Span; } }
  }
}
