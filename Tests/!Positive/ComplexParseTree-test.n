// REFERENCE: ComplexParseTree-syntax
using Nitra;
using System.Console;

module Program
{
  Main() : void
  {
    def source = SourceSnapshot("a(b, c)");
    def result = M.Start.Parse(source);
    if (result.IsSuccess)
    {
      def ast = MParseTree.Start.CreateParseTree(result);
      WriteLine(ast);
    }
    else
      WriteLine("Parse failed");
  }
}
/*
BEGIN-OUTPUT
a(b, c)
END-OUTPUT
*/
