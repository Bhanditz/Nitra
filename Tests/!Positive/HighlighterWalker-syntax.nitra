syntax module Calc
{
  using Whitespaces;
  using StandardSpanClasses;

  [SpanClass(Number)]
  regex Number = ['0'..'9']+;

  syntax Expr
  {
    | Number
    | Group = "(" Expr ")"
  precedence Sum:
    | Sum   = Expr "+" Expr;
  precedence Mul:
    | Mul   = Expr "*" Expr;
  precedence Or:
    | Or    = Expr "or"  Expr;
  precedence And:
    | And   = Expr "and" Expr;
  }

  [StartRule, ExplicitSpaces] syntax Start = s Expr !Any;
}
