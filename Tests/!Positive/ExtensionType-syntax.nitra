syntax module M
{
  using Whitespaces;

  [StartRule]
  syntax Start = A !Any
  {
    Value() : int = A.Value();
  }
  
  marker K1;

  syntax A
  {
    Value() : int;

    | X1 = "x1"          { override Value = 1; }
    | X2 = "x2"          { override Value = 2; }
  precedence Sum:
    | Y = P=(A "+") A    { override Value = P.Item1.Value() + A.Value(); }
  precedence Mult:
    | Z = Q=(K1 A) "*" A { override Value = Q.Value() + A.Value(); }
  }
}