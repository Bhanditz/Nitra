using Nitra;

syntax module RecursionTest2
{
  using Nitra.Core;

  [StartRule, ExplicitSpaces]
  syntax Start = s A !Any
  {
    Foo() : int = A.Foo(null);
  }

  syntax A
  {
    Foo(_owner : RecursionTest2ParseTree.A) : int;

    | X = "x" A
      {
        override Foo = A.Foo(this);
      }

    | Y = "y"
      {
        override Foo = _owner?.Foo(null);
      }
  }
}
