using Nitra;
using System.Console;

module Program
{
  Main() : void
  {
    def checkResult(result)
    {
      if (result.IsSuccess)
        WriteLine("OK");
      else
        WriteLine("FAIL");
    }

    def parserHost = ParserHost();

    checkResult(M.Start(SourceSnapshot(""), parserHost));
    checkResult(M.Start(SourceSnapshot("\u0000"), parserHost));
  }
}
