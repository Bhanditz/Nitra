// REFERENCE: StaticExtension-syntax
using Nitra;
using System.Console;

module Program
{
  Main() : void
  {
    def source = SourceSnapshot("a b c d");
    def result = BaseModule.Start(source);
    if (result.IsSuccess)
      WriteLine("OK");
    else
      WriteLine("FAIL");
  }
}
