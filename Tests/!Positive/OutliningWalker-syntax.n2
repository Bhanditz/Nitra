syntax module Blocks
{
  using Whitespaces;
  using Identifiers;
  using Outline;

  syntax Block
  {
    | Identifier = IdentifierBody;
    | Curly      = "{" outline_impl  (Block; ",")* outline_end "}";
    | Square     = "[" outline_begin (Block; ",")* outline_end "]";
    | Round      = "(" outline_begin (Block; ",")* outline_end ")";
  }
  
  [StartRule, ExplicitSpaces] syntax Start = s Block !Any;
}
