namespace Nitra
{
  syntax module Test
  {
    syntax IdentifierBody = "";
    
    token Identifier = !Keyword IdentifierBody
    {
      token Keyword = KeywordToken !IdentifierBody
      {
        regex KeywordToken = "true" | "false" | "null";
        
        map Foo
        {
          KeywordToken -> X;
        }
      }
    }
  }
}

map syntax Nitra.Test.Identifier.Keyword -> Foo
{
  KeywordToken -> X;
}

ast Foo { X : string; }
