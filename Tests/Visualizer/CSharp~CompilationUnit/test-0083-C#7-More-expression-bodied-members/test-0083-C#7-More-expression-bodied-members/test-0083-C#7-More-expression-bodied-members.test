using System;

class A
{
  int _x = 42;

  public A(int i) => Console.WriteLine($"B({i})");

  ~A() => Console.WriteLine("~A()");

  public ref int this[int i] => ref _x;
  public int this[int a, int b] => _x;
}

class B : A
{
  public B(int i) : base(i) => Console.WriteLine($"B({i})");
  ~B() => Console.WriteLine("~B()");
}
