using N2;

syntax module ModifierParser
{
  using N2Parser;

  syntax Modifiers = Modifier*;

  syntax Modifier;

  syntax New       is Modifier = "new";
  syntax Public    is Modifier = "public";
  syntax Protected is Modifier = "protected";
  syntax Internal  is Modifier = "internal";
  syntax Private   is Modifier = "private";
  syntax Partial   is Modifier = "partial";
  syntax Virtual   is Modifier = "virtual";
  syntax Volatile  is Modifier = "volatile";
  syntax Static    is Modifier = "static";
  syntax Readonly  is Modifier = "readonly";
  syntax Sealed    is Modifier = "sealed";
  syntax Override  is Modifier = "override";
  syntax Abstract  is Modifier = "abstract";
  syntax Extern    is Modifier = "extern";
  syntax Unsafe    is Modifier = "unsafe";
}
